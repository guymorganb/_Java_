package Strings;

public class reverseEachWord {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "Hello, Im learning Java.";
		String temp = "";
		for(int i = str.length()-1; i <= 18; i--) {
			temp = str.charAt(i) + temp;
			
		}
		System.out.print(temp);
	}

}

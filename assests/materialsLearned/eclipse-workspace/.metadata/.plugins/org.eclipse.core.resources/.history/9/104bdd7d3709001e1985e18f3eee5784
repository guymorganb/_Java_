package ObjectOrientedPrograming;

public class PolynomialUse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Polynomial p1 = new Polynomial();
		p1.add(2, 3);
		p1.add(1, 5);
		p1.add(0, 1);
		p1.print();
		
		Polynomial p2 = new Polynomial();
		p2.add(2, 5);
		p2.add(1, 3);
		p2.print();
		
	}

}

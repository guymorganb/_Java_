

package inheritance;
//to indicate inheritance, use 'extends' keyword
public class car extends Vehicle{
	
	int numDoors;
	
	public void printCar() {
		System.out.println("car " + "color "+ getColor() +" maxspeed "+ maxSpeed+ " numDoors " + numDoors);
	}
	
	public void print() {
		System.out.println("This is an example of function overriding, print() is defined in vehicle and car, but car.print() will override and run first if it exists");
	}
}

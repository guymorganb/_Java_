package ObjectOrientedPrograming;

public class FractionUse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Fraction f1 = new Fraction(4,6);
		
		f1.print();
		
		f1.increment(3);
		
		f1.print();

		Fraction f2 = new Fraction(2,3);
		
		f1.addFractions(f2);
	}

}
